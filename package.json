{
  "name": "realstates",
  "version": "1.0.0",
  "description": "Lists and Inserts Real States on a MongoDB database, using node-js and typescript",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Antonio Casagrande",
    "email": "antonioalvescasagrande@gmail.com",
    "url": "https://github.com/antonioacg"
  },
  "engines": {
    "node": "^8.9.1",
    "npm": "5.5.1"
  },
  "scripts": {
    "heroku-prebuild": "npm install --only=dev && npm install @types/chai && node_modules/.bin/tsc -p . && npm run makeDevDoc",
    "dev": "cross-env NODE_ENV='devel' && nodemon --exec ts-node src/index.ts",
    "prod": "cross-env NODE_ENV='production' && nodemon --exec ts-node src/index.ts",
    "build": "tsc -p . && apidoc -i src -o build/public/doc",
    "buildTest": "tsc -p .",
    "makeDoc": "node_modules/.bin/apidoc -i src -o src/public/doc",
    "makeDevDoc": "node_modules/.bin/apidoc -i src -o build/public/doc",
    "start": "node build/index.js",
    "test": "node_modules/.bin/mocha --opts src/test/mocha.opts",
    "test:watch": "cross-env PORT='3001' NODE_ENV='devel' && node_modules/.bin/mocha --opts src/test/mocha.opts --watch"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "4.0.5",
    "@types/chai-http": "3.0.4",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.3",
    "@types/express": "4.11.1",
    "@types/express-jwt": "^0.0.38",
    "@types/express-session": "^1.15.6",
    "@types/helmet": "^0.0.37",
    "@types/jsonwebtoken": "7.2.5",
    "@types/mocha": "2.2.44",
    "@types/mongodb": "^2.2.15",
    "@types/mongoose": "^4.7.27",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.53",
    "@types/passport": "0.4.1",
    "@types/supertest": "^2.0.4",
    "apidoc": "^0.17.6",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "cross-env": "5.1.3",
    "eslint": "^4.11.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express-basic-auth": "1.1.4",
    "jsonwebtoken": "8.2.0",
    "mocha": "4.0.1",
    "prettier": "^1.8.2",
    "supertest": "^3.0.0",
    "ts-node": "3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@types/chai-as-promised": "7.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "mongoose": "^4.13.4",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "passport": "0.4.0",
    "passport-local": "^1.0.0"
  }
}
